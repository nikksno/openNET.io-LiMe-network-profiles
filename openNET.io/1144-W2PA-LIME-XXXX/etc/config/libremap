config libremap 'settings'
        list api_url 'http://libremap.net/api'
        option community 'openNET.io'
	option community_lat '45.464161'
	option community_lon '9.190336'
        option fetch_by_name '1'

config plugin 'location'
        option enabled '1'
        option elev '0'
        option latitude '45.464161'
        option longitude '9.190336'

config plugin 'system'
        option enabled '1'

config plugin 'wireless'
        option enabled '1'
